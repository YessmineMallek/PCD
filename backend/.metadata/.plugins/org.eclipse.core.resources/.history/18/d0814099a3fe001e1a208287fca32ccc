FROM eclipse-temurin:17-jdk-alpine
COPY target/*.jar trainTruck.jar
ENTRYPOINT ["java","-jar","trainTruck.jar"]

FROM postgres:latest

# Copy the SQL file into the container
COPY your_sql_file.sql /docker-entrypoint-initdb.d/

# Grant permissions to the SQL file
RUN chmod 755 /docker-entrypoint-initdb.d/your_sql_file.sql